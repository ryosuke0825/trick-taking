{
  "name": "be",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18.17"
  },
  "scripts": {
    "dev": "tsx src/server.ts",
    "gen": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src --ts_proto_opt=esModuleInterop=true,outputServices=grpc-js,useExactTypes=false --proto_path=proto proto/infra.proto",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.8.0",
    "dotenv": "^16.4.5",
    "mysql2": "^3.14.4"
  },
  "devDependencies": {
    "ts-proto": "^2.7.7",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
